    "tags": ["date","datetime","format"],
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^5.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Heigl",
            "email": "andreas@heigl.org"
        }
    ],
    "require": {
        "org_heigl/filefinder": "^1.0"
    },
    "autoload": {
        "psr-4" : {
